@{
    ViewBag.Title = "";
    Layout = "~/Views/Layouts/_Admin.cshtml";
}

<div id="maincontainer">
  <div id="topsection" class="ui-state-default">
    <div class="innertube">
        @Html.Partial("~/Views/Shared/_status_panel.cshtml")
        @Html.Partial("_logout")
        @Html.Partial("_theme_select")
    </div>
  </div>
  <div id="contentwrapper">
    <div id="contentcolumn">
      <div class="innertube"></div>
    </div>
  </div>
  <div id="leftcolumn">
    <div class="innertube">
        @Html.Partial("_menu")
    </div>
  </div>
</div>
<div id="error-dialog" title="Internal Server Error">
  <div id="error_dialog"></div>
</div>
<div id="progress_status">
    @Html.Partial("_progress_panel")
</div>
