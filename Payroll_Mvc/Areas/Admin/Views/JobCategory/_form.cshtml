@using Domain.Model;

@model Jobcategory

<form id="@ViewBag.form_id" class="save_form ui-widget-content ui-widget ui-corner-all" action=".">
    <table>
        <tr>
            <td valign="top" class="lbl">
                <label for="name" class="label">Name</label>
            </td>
            <td>
                <input id="id_name" name="name" type="text" class="text" placeholder="enter name" title="enter name" x-webkit-speech="x-webkit-speech" value="@Model.Name" />
            </td>
        </tr>
        <tr>
            <td></td>
            <td>
                <div class="row_button">
                    @{ Html.RenderPartial("_save_button"); }
                    @{ Html.RenderPartial("_cancel_button"); }
                </div>
            </td>
        </tr>
    </table>
</form>
